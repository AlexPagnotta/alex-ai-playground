{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "npm run lint.eslint && npm run lint.types",
    "lint.eslint": "eslint --color --cache --cache-location 'node_modules/.cache/eslint' src --ext .js,.jsx,.ts,.tsx",
    "lint.types": "tsc -b --noEmit",
    "format": "prettier --write 'src/**/*.{json,md,mdx,css,js,jsx,ts,tsx}' && npm run lint -- --fix"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "cva": "^1.0.0-beta.4",
    "eslint-config-react-alexpagnotta": "^1.1.1",
    "lucide-react": "^0.525.0",
    "prettier-config-alexpagnotta": "^1.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "wouter": "^3.7.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "globals": "^16.3.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
